{
  "name": "@node-steam/market-pricing",
  "version": "2.0.1",
  "description": "Module to access the Steam Market Pricing API",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "homepage": "https://github.com/node-steam/market-pricing",
  "bugs": "https://github.com/node-steam/market-pricing/issues",
  "repository": {
    "url": "https://github.com/node-steam/market-pricing",
    "type": "git"
  },
  "author": "N|Steam",
  "keywords": [
    "api",
    "es6",
    "es7",
    "market-pricing",
    "market",
    "price",
    "pricing",
    "promises",
    "skin",
    "skins",
    "steam-api",
    "steam-market",
    "steam",
    "typescript"
  ],
  "files": [
    "lib/*.js",
    "lib/*.d.ts",
    "example/*.js"
  ],
  "contributors": [
    "Silas Rech <silas.rech@protonmail.com> (https://lenovouser.me)"
  ],
  "runkitExampleFilename": "example/runkit.js",
  "engines": {
    "node": ">=6.0.0"
  },
  "license": "MIT",
  "scripts": {
    "compile": "tsc -p .",
    "test": "yarn compile && ava",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "docs": "typedoc src/index.ts",
    "prepublish": "yarn lint && yarn test && yarn docs"
  },
  "ava": {
    "files": [
      "test/mock/*.js",
      "test/mock/**/*.js"
    ],
    "source": [
      "lib/*.js"
    ],
    "concurrency": 5,
    "verbose": true,
    "failFast": true,
    "failWithoutAssertions": false,
    "powerAssert": false,
    "require": [
      "babel-register",
      "babel-polyfill"
    ]
  },
  "devDependencies": {
    "@node-steam/data": "1.1.0",
    "@types/async": "^2.0.45",
    "@types/bluebird": "^3.5.19",
    "@types/form-data": "2.2.1",
    "@types/node": "8.5.2",
    "@types/request": "2.0.9",
    "app-module-path": "^2.2.0",
    "ava": "0.24.0",
    "babel-core": "^6.24.1",
    "babel-polyfill": "^6.23.0",
    "babel-register": "^6.24.1",
    "nock": "9.1.5",
    "typescript": "^2.6.2"
  },
  "dependencies": {
    "async": "^2.6.0",
    "bluebird": "^3.5.1",
    "request": "^2.83.0"
  }
}
