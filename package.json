{
  "name": "@node-steam/market-pricing",
  "version": "1.1.2",
  "description": "Module to access the Steam Market Pricing API",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "homepage": "https://github.com/node-steam/market-pricing",
  "bugs": "https://github.com/node-steam/market-pricing/issues",
  "repository": {
    "url": "https://github.com/node-steam/market-pricing",
    "type": "git"
  },
  "author": "N|Steam",
  "keywords": [
    "steam",
    "steam-api",
    "price",
    "pricing",
    "skin",
    "skins",
    "promises",
    "typescript",
    "es6",
    "es7"
  ],
  "files": [
    "lib/*.js",
    "lib/*.d.ts",
    "example/*.js"
  ],
  "contributors": [
    "Silas Rech <silas.rech@protonmail.com> (https://lenovouser.me)"
  ],
  "runkitExampleFilename": "example/runkit.js",
  "engines": {
    "node": ">=6.0.0"
  },
  "license": "MIT",
  "scripts": {
    "compile": "tsc -p .",
    "test": "yarn compile && ava",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "docs": "typedoc src/index.ts",
    "prepublish": "yarn lint && yarn test && yarn docs"
  },
  "ava": {
    "files": [
      "test/mock/*.js",
      "test/mock/**/*.js"
    ],
    "source": [
      "lib/*.js"
    ],
    "concurrency": 5,
    "verbose": true,
    "failFast": true,
    "failWithoutAssertions": false,
    "powerAssert": false,
    "require": [
      "babel-register",
      "babel-polyfill"
    ]
  },
  "devDependencies": {
    "@types/async": "^2.0.40",
    "@types/bluebird-global": "^3.5.2",
    "@types/form-data": "^0.0.33",
    "@types/node": "^7.0.18",
    "app-module-path": "^2.2.0",
    "ava": "^0.19.1",
    "babel-core": "^6.24.1",
    "babel-polyfill": "^6.23.0",
    "babel-register": "^6.24.1",
    "eslint": "^3.19.0",
    "eslint-config-google": "^0.7.1",
    "nock": "^9.0.13",
    "typescript": "^2.3.2",
    "typescript-eslint-parser": "^2.1.0"
  },
  "dependencies": {
    "async": "^2.4.0",
    "bluebird": "^3.5.0",
    "request": "^2.81.0"
  }
}
